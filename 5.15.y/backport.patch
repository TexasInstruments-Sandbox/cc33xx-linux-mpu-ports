-	case WLAN_AUTH_LEAP:
-	case WLAN_AUTH_FT:
-	case WLAN_AUTH_SAE:
-	case WLAN_AUTH_FILS_SK:
-	case WLAN_AUTH_FILS_SK_PFS:
-	case WLAN_AUTH_FILS_PK:
