-	priv->membase = membase;
-	priv->line = -ENODEV;
-	priv->latency = PM_QOS_CPU_LATENCY_DEFAULT_VALUE;
-	priv->calc_latency = PM_QOS_CPU_LATENCY_DEFAULT_VALUE;
