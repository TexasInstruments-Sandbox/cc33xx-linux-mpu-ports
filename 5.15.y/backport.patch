-		int aci = (pos[0] >> 5) & 0x03;
-		int acm = (pos[0] >> 4) & 0x01;
-		bool uapsd = false;
-
-		switch (aci) {
-		case 1: /* AC_BK */
-			ac = IEEE80211_AC_BK;
-			if (acm)
-				sdata->wmm_acm |= BIT(1) | BIT(2); /* BK/- */
-			if (uapsd_queues & IEEE80211_WMM_IE_STA_QOSINFO_AC_BK)
-				uapsd = true;
