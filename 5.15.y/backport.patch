-		    !ifmgd->have_beacon) {
-			drv_mgd_prepare_tx(sdata->local, sdata, &info);
-		}
-
-		ieee80211_send_deauth_disassoc(sdata, ifmgd->bssid,
-					       ifmgd->bssid, stype, reason,
-					       tx, frame_buf);
-	}
