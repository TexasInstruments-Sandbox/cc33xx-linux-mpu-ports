-	if (!local->ops->conf_tx)
-		return;
-
-	if (local->hw.queues < IEEE80211_NUM_ACS)
-		return;
-
-	memset(&qparam, 0, sizeof(qparam));
-
-	rcu_read_lock();
-	chanctx_conf = rcu_dereference(sdata->vif.chanctx_conf);
