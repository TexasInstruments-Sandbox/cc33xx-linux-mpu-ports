-		auth_alg = WLAN_AUTH_OPEN;
-		break;
-	case NL80211_AUTHTYPE_SHARED_KEY:
-		if (fips_enabled)
-			return -EOPNOTSUPP;
-		auth_alg = WLAN_AUTH_SHARED_KEY;
