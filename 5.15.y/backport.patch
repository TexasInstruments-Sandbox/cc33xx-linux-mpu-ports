-
-	skb = ieee80211_build_probe_req(sdata, sdata->vif.addr, cbss->bssid,
-					(u32) -1, cbss->channel,
-					ssid + 2, ssid_len,
-					NULL, 0, IEEE80211_PROBE_FLAG_DIRECTED);
-	rcu_read_unlock();
-
-	return skb;
-}
