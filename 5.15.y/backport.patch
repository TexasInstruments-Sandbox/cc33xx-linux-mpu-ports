-void ieee80211_dynamic_ps_timer(struct timer_list *t)
-{
-	struct ieee80211_local *local = from_timer(local, t, dynamic_ps_timer);
-
-	ieee80211_queue_work(&local->hw, &local->dynamic_ps_enable_work);
