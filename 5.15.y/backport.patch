-	    !(info->control.hw_key->flags & IEEE80211_KEY_FLAG_GENERATE_IV) &&
-	    !(info->control.hw_key->flags & IEEE80211_KEY_FLAG_PUT_IV_SPACE) &&
-	    !((info->control.hw_key->flags &
-	       IEEE80211_KEY_FLAG_GENERATE_IV_MGMT) &&
