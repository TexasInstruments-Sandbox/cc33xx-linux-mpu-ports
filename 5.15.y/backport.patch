-	ieee80211_bss_info_change_notify(sdata, BSS_CHANGED_BSSID);
-	ifmgd->auth_data = NULL;
-	mutex_lock(&sdata->local->mtx);
-	ieee80211_vif_release_channel(sdata);
-	mutex_unlock(&sdata->local->mtx);
-	kfree(auth_data);
