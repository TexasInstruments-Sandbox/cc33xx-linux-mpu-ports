-			for (ac = 0; ac < n_acs; ac++) {
-				int ac_queue = sdata->vif.hw_queue[ac];
-
-				if (ac_queue == i ||
-				    sdata->vif.cab_queue == i)
-					__ieee80211_wake_txqs(sdata, ac);
-			}
-		}
-		spin_lock_irqsave(&local->queue_stop_reason_lock, *flags);
