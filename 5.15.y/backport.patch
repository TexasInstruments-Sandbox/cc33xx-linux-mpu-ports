-		if (auth_alg == WLAN_AUTH_SAE &&
-		    (status_code == WLAN_STATUS_ANTI_CLOG_REQUIRED ||
-		     (auth_transaction == 1 &&
-		      (status_code == WLAN_STATUS_SAE_HASH_TO_ELEMENT ||
