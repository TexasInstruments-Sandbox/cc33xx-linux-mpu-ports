-static bool ieee80211_rx_our_beacon(const u8 *tx_bssid,
-				    struct cfg80211_bss *bss)
-{
-	if (ether_addr_equal(tx_bssid, bss->bssid))
-		return true;
-	if (!bss->transmitted_bss)
-		return false;
-	return ether_addr_equal(tx_bssid, bss->transmitted_bss->bssid);
-}
