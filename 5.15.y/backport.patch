-		       req->ie, req->ie_len);
-		auth_data->data_len += req->ie_len;
-	}
-
-	if (req->key && req->key_len) {
-		auth_data->key_len = req->key_len;
-		auth_data->key_idx = req->key_idx;
-		memcpy(auth_data->key, req->key, req->key_len);
