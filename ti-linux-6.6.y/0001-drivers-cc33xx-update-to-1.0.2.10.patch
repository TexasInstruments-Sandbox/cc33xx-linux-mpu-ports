+		case 120:
+		case 136:
+		case 153:
+		case 169:
+			return 1;
+		case 44:
+		case 60:
+		case 108:
+		case 124:
+		case 140:
+		case 157:
+		case 173:
+			return 2;
+		case 48:
+		case 64:
+		case 112:
+		case 128:
+		case 144:
+		case 161:
+		case 177:
+			return 3;
+		default:
+			return 0xff;
+		}
+	}
+
+	if (band == NL80211_BAND_2GHZ) {
+		switch (channel) {
+		case 9:
+		case 5:
+			return 0; 
+		case 1:
+			return 0;
+		case 13:
+			return 1;
+		default:
+			return 0;
+		}
+	}
+
+	return 0xff;
+}
