+	if (ret < 0) {
+		cc33xx_warning("illegal value in ps_mode");
+		return -EINVAL;
+	}
+
+	if (value < 0 || value > 2) {
+		cc33xx_warning("ps_mode should be either 0 or 1 or 2");
