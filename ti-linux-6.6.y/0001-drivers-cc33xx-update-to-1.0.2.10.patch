+		return -EINVAL;
+
+	sdio_claim_host(sdiodev->func);
+
+	ret = btti_sdio_enable_func_interrupt(sdiodev, INTRPT_ENABLE_MASk);
+
+	sdio_release_host(sdiodev->func);
